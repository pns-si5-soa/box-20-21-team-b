groups:
- name: rocket-monitoring
  rules:

  - alert: taking_off
    expr: boxb_module_metrics_isRunning{instance="module-metrics-booster-1:80",job="module-metrics"} == 0
    labels:
      severity: safe
    annotations:
      summary: "Rocket {{ $labels.Id }} fired"
      description: "{{ $labels.Id }} module has speed {{ $labels.Speed }}"
